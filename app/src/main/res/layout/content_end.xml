<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    app:layout_behavior="@string/appbar_scrolling_view_behavior" tools:showIn="@layout/activity_end"
    tools:context="com.example.administrator.myapplication.EndActivity">

    <TextView
        android:id="@+id/result"
        android:text="YOU WIN!!!"
        android:layout_centerHorizontal="true"
        android:textSize="30dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />



    <TableLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/result"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="30dp">


        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/blank"
                android:layout_marginTop="10dp"
                android:layout_column="0" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/self"
                android:text="自己"
                android:layout_below="@+id/result"
                android:textSize="30dp"
                android:layout_marginTop="10dp"
                android:layout_marginLeft="40dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/other"
                android:text="对方"
                android:layout_below="@+id/result"
                android:textSize="30dp"
                android:layout_marginTop="10dp"
                android:layout_marginLeft="40dp" />
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/accuracy"
                android:text="正确率"
                android:textSize="30dp"
                android:layout_marginTop="10dp"
                android:layout_column="0" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/selfAccuracy"
                android:text="100%"
                android:layout_below="@+id/result"
                android:textSize="30dp"
                android:layout_marginTop="10dp"
                android:layout_marginLeft="40dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/otherAccuracy"
                android:text="20%"
                android:layout_below="@+id/result"
                android:textSize="30dp"
                android:layout_marginTop="10dp"
                android:layout_marginLeft="40dp" />
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/Time"
                android:text="耗    时"
                android:textSize="30dp"
                android:layout_marginTop="10dp"
                android:layout_column="0" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/selfTime"
                android:text="10s"
                android:layout_below="@+id/result"
                android:textSize="30dp"
                android:layout_marginTop="10dp"
                android:layout_marginLeft="40dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/otherTime"
                android:text="20s"
                android:layout_below="@+id/result"
                android:textSize="30dp"
                android:layout_marginTop="10dp"
                android:layout_marginLeft="40dp" />
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <Button
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="返回主页"
                android:id="@+id/button"
                android:onClick="gohome"
                android:layout_marginTop="10dp"
                android:layout_marginLeft="40dp"
                android:layout_column="1" />
        </TableRow>
    </TableLayout>


</RelativeLayout>
